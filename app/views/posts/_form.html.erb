<%= form_for @post do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <br />

  <%= f.label :date %>
  <%= f.text_field :date %>
  <br />

  <%= f.label :body %>
  <%= f.text_field :body %>
  <br />
  
  <%= f.label :completed %>
  <%= f.check_box :completed %>


  <%= f.submit %>
<% end %>

<br />
<%= link_to 'Main Page', posts_path %>